{
  "name": "Frivacy",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Manage your facebook Privacy",
  "browser_action": {
    "default_icon": "images/close.png",
	"default_popup": "popup.html",
	"default_title": "Click here to submit your data"
  },
  "permissions": [
    "http://*.facebook.com/*",
	"https://*.facebook.com/*",
	"https://*.lersais.exp.sis.pitt.edu/*",
	"tabs"
  ]
}